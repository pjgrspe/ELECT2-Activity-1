@{
    ViewData["Title"] = "Hello World";
}
<div class="d-flex justify-content-center align-items-center vh-100">
    <h1 class="text-center display-4">Hello World</h1>
</div>

<script src="https://cdn.jsdelivr.net/npm/sweetalert2@10"></script>
<script>
